<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
         List of default servers to be used with this build. Information about the server
         Can be included in parenthesis after the server name. The server name here is used
         to set the server url. If you have a configuration for a specific server, put
         it in /res/raw/config.yaml
    -->
    <string-array name="servers">
        <item>https://lausd.mobilizingcs.org/</item>
    </string-array>

    <!--
         If false, the user will not be able to change the server and the first
         server in the server list will be used.
    -->
    <item name="allow_custom_server" type="bool">false</item>

    <!-- If false, the user will not be able to see the register link. -->
    <item name="allow_user_registration" type="bool">false</item>
</resources>